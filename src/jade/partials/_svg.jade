mixin svg()
	svg(style="display: none", xmlns="http://www.w3.org/2000/svg")
		symbol#larr-icon(width="20", height="20", viewbox="0 0 6 6")
			path(d="M 1 3 L 5 3 M 1 3 L 2.5 1.5 M 1 3 L 2.5 4.5")
		symbol#close-icon(width="20", height="20", viewbox="0 0 6 6")
			path(d="M 1 1 L 5 5 M 5 1 L 1 5")
		symbol#zoom-out-icon(width="20", height="20", viewbox="0 0 7 7")
			path(d="M 1 3 a 2 2 0 1 0 4 0 a 2 2 0 1 0 -4 0 M 4.5 4.5 L 6 6 M 2.5 3 L 3.5 3")
		symbol#spinner(width="20", height="20", viewBox="0, 0, 30, 30")
			path(d="M 15 25 A 5 5 0 1 1 15 5", stroke="#eceff1", stroke-width="1", fill="none", stroke-linecap="round")
